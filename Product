package dominio;

public class Product {
	
	private String ID,productName , category , date , saw , description;
	private int price;
	
	public Product(String ID , String productName, String category, String date,int price,String description) {
		super();
		this.ID=ID;
		this.productName = productName;
		this.category = category;
		this.date = date;
		this.saw = "No";
		this.price=price;
		this.description=description;
	}
	
	
	@Override
	public String toString() {
		return "Product [ID=" + ID + ", productName=" + productName + ", category=" + category + ", date=" + date
				+ ", saw=" + saw + "]";
	}


	public String getProductName() {
		return productName;
	}

	public void setProductName(String nameProduct) {
		this.productName = nameProduct;
	}

	public String getCategory() {
		return category;
	}

	public void setCategory(String category) {
		this.category = category;
	}

	public String getDate() {
		return date;
	}

	public void setDate(String date) {
		this.date = date;
	}

	public String getSaw() {
		return saw;
	}

	public void setSaw(String saw) {
		this.saw = saw;
	}

	public String getID() {
		return ID;
	}

	public void setID(String iD) {
		ID = iD;
	}


	public String getDescription() {
		return description;
	}


	public void setDescription(String description) {
		this.description = description;
	}


	public int getPrice() {
		return price;
	}


	public void setPrice(int price) {
		this.price = price;
	}
	
	
}
